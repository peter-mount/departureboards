{
  "name": "departureboards",
  "version": "0.4.0",
  "private": false,
  "publishConfig": {
        "registry": "https://nexus.area51.onl/repository/npm-private/"
  },
  "scripts": {
    "build": "npm run babel && npm run webpack",
    "babel": "babel --plugins transform-es2015-modules-umd src --ignore __tests__ --out-dir ./build",
    "webpack": "webpack --config webpack.config.js && cp -v public/* dist",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix",
    "lint:watch": "esw -w src/**",
    "run": "npm run build && npm run dev",
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --inline --history-api-fallback"
  },
  "dependencies": {
    "area51-apilock": "^0.1.2",
    "area51-eucookie": "^0.1.1",
    "area51-timepagination": "^0.1.1",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.0",
    "react-bootstrap-typeahead": "^1.4.0",
    "react-dom": "^15.6.1",
    "react-moment": "^0.6.0",
    "react-prop-types": "^0.4.0",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "uktrain-stationsearch": "^0.1.0"
  },
  "devDependencies": {
    "file-loader": "^0.11.2",
    "polished": "^1.3.0",
    "styled-components": "^2.1.1",
    "uglifyjs-webpack-plugin": "^0.4.6"
  }
}
