  "scripts": {
    "build": "npm run babel && npm run webpack",
    "babel": "babel --plugins transform-es2015-modules-umd src --ignore __tests__ --out-dir ./build",
    "webpack": "webpack --config webpack.config.js && cp -v public/* dist",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix",
    "lint:watch": "esw -w src/**",
    "run": "npm run build && npm run dev",
    "dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --inline --history-api-fallback"
  },
